# Development Environment Configuration
NODE_ENV=development
PORT=8000
HOST=localhost

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# File Upload Configuration
MAX_FILE_SIZE=10485760

# AI Models Configuration
VIT_ENABLED=true
SWIN_ENABLED=true
BLIP_ENABLED=true
CLIP_ENABLED=true
YOLO_ENABLED=true
YOLO_MODEL_PATH=yolo11m.pt

# Language Model Configuration
LLM_ENABLED=true
GROQ_API_KEY=your_groq_api_key_here
LLM_MODEL=mixtral-8x7b-32768

# Detection Configuration
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=100

# Logging Configuration
LOG_LEVEL=debug
LOG_FORMAT=dev

# External APIs (Optional for development)
NUTRITION_API_ENABLED=false
NUTRITION_API_URL=
NUTRITION_API_KEY=

FOOD_DB_ENABLED=false
FOOD_DB_URL=
FOOD_DB_API_KEY=

# Python Models Service Configuration
# Set this to your deployed Python models service URL on Render
# Example: https://your-python-models-service.onrender.com
PYTHON_MODELS_URL=https://food-detection-models.onrender.com
